{
  "name": "goldman",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Felipe.DS <Felipe.DS@outlook.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn install && yarn build && yarn sequelize db:migrate && node dist/server.js",
    "build": "babel src -d dist --copy-files",
    "queue": "nodemon src/queue.js",
    "dev:debug": "nodemon --inspect src/server.js",
    "pretest": "set NODE_ENV=test&&sequelize db:migrate",
    "test": "set NODE_ENV=test&&jest --runInBand",
    "posttest": "set NODE_ENV=test&&sequelize db:migrate:undo:all",
    "pretest:coverage": "sequelize db:migrate",
    "test:coverage": "jest --runInBand --coverage",
    "posttest:coverage": "sequelize db:migrate:undo:all",
    "pretest:coveralls": "sequelize db:migrate",
    "test:coveralls": "jest --runInBand --coverage && coveralls < __tests__/coverage/lcov.info",
    "posttest:coveralls": "sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "bcryptjs": "^2.4.3",
    "bee-queue": "^1.2.2",
    "cors": "^2.8.5",
    "date-fns": "^2.6.0",
    "dot-prop": "5.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "mockdate": "^3.0.2",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.3.1",
    "nodemailer-express-handlebars": "^3.1.0",
    "sequelize": "^5.21.1",
    "sequelize-cli": "^5.5.1",
    "youch": "^2.0.10",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@types/jest": "26.0.13",
    "babel-jest": "^26.3.0",
    "concurrently": "^5.0.2",
    "coveralls": "^3.1.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "factory-girl": "^5.0.4",
    "faker": "^5.1.0",
    "faker-br": "^0.3.0",
    "jest": "^26.4.2",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "sqlite3": "^5.0.0",
    "sucrase": "3.15.0",
    "supertest": "^4.0.2"
  }
}
